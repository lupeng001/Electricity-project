<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@mipmap/bg_myf"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_myshep"
        android:layout_marginTop="@dimen/dp_95"
        >
        <TextView
            android:id="@+id/text_head_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="爱听民谣的小朋友"
            android:layout_marginTop="@dimen/dp_25"
            android:textSize="14sp"
            android:textColor="#666666"
            />
        <RelativeLayout
            android:id="@+id/re1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:layout_below="@id/text_head_name"
            android:layout_marginTop="@dimen/dp_10"
            >
            <ImageView
                android:id="@+id/img_information"
                android:layout_width="@dimen/dp_12"
                android:layout_height="@dimen/dp_12"
                android:background="@mipmap/my_icon_information"
                android:layout_marginLeft="@dimen/dp_12"
                android:layout_alignParentBottom="true"
                />
            <TextView
                android:id="@+id/text_information_my"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="@dimen/dp_39"
                android:text="个人资料"
                android:textSize="14sp"
                android:textColor="#666666"
                android:layout_marginBottom="@dimen/dp_3"
                />
        </RelativeLayout>
        <View
            android:layout_width="@dimen/dp_133"
            android:layout_height="@dimen/dp_1"
            android:background="#dedede"
            android:layout_below="@id/re1"
            android:layout_marginLeft="@dimen/dp_35"
            />
        <RelativeLayout
            android:id="@+id/re2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:layout_below="@id/re1"
            android:layout_marginTop="@dimen/dp_7"
            >
            <ImageView
                android:id="@+id/img_circle_my"
                android:layout_width="@dimen/dp_12"
                android:layout_height="@dimen/dp_12"
                android:background="@mipmap/my_icon_circle"
                android:layout_marginLeft="@dimen/dp_12"
                android:layout_alignParentBottom="true"
                />
            <TextView
                android:id="@+id/text_circle_my"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="@dimen/dp_36"
                android:text="我的圈子"
                android:textSize="14sp"
                android:textColor="#666666"
                android:layout_marginBottom="@dimen/dp_3"
                />
        </RelativeLayout>
        <View
            android:layout_width="@dimen/dp_120"
            android:layout_height="@dimen/dp_1"
            android:background="#dedede"
            android:layout_below="@id/re2"
            android:layout_marginLeft="@dimen/dp_36"
            />
        <RelativeLayout
            android:id="@+id/re3"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:layout_below="@id/re2"
            android:layout_marginTop="@dimen/dp_7"
            >
            <ImageView
                android:id="@+id/img_food_my"
                android:layout_width="@dimen/dp_12"
                android:layout_height="@dimen/dp_12"
                android:background="@mipmap/my_icon_foot"
                android:layout_marginLeft="@dimen/dp_12"
                android:layout_alignParentBottom="true"
                />
            <TextView
                android:id="@+id/text_foot_my"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="@dimen/dp_36"
                android:text="我的足迹"
                android:textSize="14sp"
                android:textColor="#666666"
                android:layout_marginBottom="@dimen/dp_3"
                />
        </RelativeLayout>
        <View
            android:layout_width="@dimen/dp_120"
            android:layout_height="@dimen/dp_1"
            android:background="#dedede"
            android:layout_below="@id/re3"
            android:layout_marginLeft="@dimen/dp_36"
            />
        <RelativeLayout
            android:id="@+id/re4"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:layout_below="@id/re3"
            android:layout_marginTop="@dimen/dp_7"
            >
            <ImageView
                android:id="@+id/img_wallet_my"
                android:layout_width="@dimen/dp_12"
                android:layout_height="@dimen/dp_12"
                android:background="@mipmap/my_icon_wallet"
                android:layout_marginLeft="@dimen/dp_12"
                android:layout_alignParentBottom="true"
                />
            <TextView
                android:id="@+id/text_wallet_my"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="@dimen/dp_36"
                android:text="我的钱包"
                android:textSize="14sp"
                android:textColor="#666666"
                android:layout_marginBottom="@dimen/dp_3"
                />
        </RelativeLayout>
        <View
            android:layout_width="@dimen/dp_120"
            android:layout_height="@dimen/dp_1"
            android:background="#dedede"
            android:layout_below="@id/re4"
            android:layout_marginLeft="@dimen/dp_36"
            />
        <RelativeLayout
            android:id="@+id/re5"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:layout_below="@id/re4"
            android:layout_marginTop="@dimen/dp_7"
            >
            <ImageView
                android:id="@+id/img_address_my"
                android:layout_width="@dimen/dp_12"
                android:layout_height="@dimen/dp_12"
                android:background="@mipmap/my_icon_address"
                android:layout_marginLeft="@dimen/dp_12"
                android:layout_alignParentBottom="true"
                />
            <TextView
                android:id="@+id/text_address_my"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="@dimen/dp_36"
                android:text="我的收货地址"
                android:textSize="14sp"
                android:textColor="#666666"
                android:layout_marginBottom="@dimen/dp_3"
                />
        </RelativeLayout>
        <View
            android:layout_width="@dimen/dp_120"
            android:layout_height="@dimen/dp_1"
            android:background="#dedede"
            android:layout_below="@id/re5"
            android:layout_marginLeft="@dimen/dp_36"
            />
    </RelativeLayout>
    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/img_my_head"
        android:layout_width="@dimen/dp_40"
        android:layout_height="@dimen/dp_40"
        android:background="@mipmap/my_head"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_70"
        />
</RelativeLayout>
